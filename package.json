{
  "name": "egg-zrole",
  "version": "4.0.0",
  "description": "casbin plugin for eggjs",
  "eggPlugin": {
    "name": "zrole"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "casbin"
  ],
  "types": "index.d.ts",
  "dependencies": {
    "casbin": "^5.30.0"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.3.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.8",
    "casbin-sequelize-adapter": "^2.7.0",
    "egg": "^3",
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12",
    "mysql2": "^3.0.0-rc.1",
    "tsd": "^0.31.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
